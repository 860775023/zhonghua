r\n      mediaStream: null\r\n    }\r\n  },\r\n  methods: {\r\n    async startCamera() {\r\n      try {\r\n        this.mediaStream = await navigator.mediaDevices.getUserMedia({\r\n          video: {\r\n            facingMode: { exact: \"environment\" }, // 强制使用后置摄像头\r\n            width: { ideal: 1280 },\r\n            height: { ideal: 720 }\r\n          }\r\n        });\r\n        \r\n        if (this.$refs.videoElement) {\r\n          this.$refs.videoElement.srcObject = this.mediaStream;\r\n        }\r\n      } catch (err) {\r\n        console.error('无法访问摄像头：', err);\r\n        alert('无法访问摄像头，请确保已授予权限并且设备已连接。');\r\n      }\r\n    },\r\n    stopCamera() {\r\n      if (this.mediaStream) {\r\n        this.mediaStream.getTracks().forEach(track => track.stop());\r\n        if (this.$refs.videoElement) {\r\n          this.$refs.videoElement.srcObject = null;\r\n        }\r\n      }\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    this.stopCamera();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.mobile-camera {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nh2 {\r\n  color: white;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.camera-container {\r\n  width: 100%;\r\n  max-width: 500px;\r\n  margin: 0 auto;\r\n  background: #000;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r\n\r\nvideo {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.controls {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 10px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  background: #1890ff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 16px;\r\n}\r\n\r\nbutton:active {\r\n  background: #096dd9;\r\n}\r\n</style> "]}]}