 \"echarts\";\n// 引用的就是中国各省份的矢量数据\nimport {zh} from \"../assets/zh.js\"; // 确保文件名和路径正确\n\nexport default {\n  mounted() {\n    let myChart = echarts.init(this.$refs.demoh);\n    echarts.registerMap(\"chinaMap\", zh); // 使用 registerMap 注册的地图名称。\n    let option = {\n      geo: { // 地理坐标系组件。地理坐标系组件用于地图的绘制\n        type: \"map\",\n        map: \"chinaMap\", // 使用 registerMap 注册的地图名称\n        roam: true, // 是否开启鼠标缩放和平移漫游。默认不开启。\n        zoom: 10, // 当前视角的缩放比例。越大比例越大\n        center: [108.956239, 34.268309], // 当前视角的中心点，用经纬度表示\n        label: {\n          show: true,\n          textStyle: {\n            color: \"#ff6600\",\n            fontSize: 10 // 字体大小\n          }\n        },\n        itemStyle: { // 地图区域的多边形 图形样式。\n          areaColor: \"#ff6600\" // 地图区域的颜色。\n        }\n      },\n      series: [\n        {\n          name: '省份',\n          type: 'map',\n          mapType: 'chinaMap',\n          roam: true,\n          label: {\n            show: true\n          },\n          data: [] // 这里可以放置需要高亮的省份数据\n        }\n      ]\n    };\n    myChart.setOption(option);\n  }\n};\n</script>\n\n<style scoped>\n\n</style>"]}]}