 ref, onMounted, onUnmounted, nextTick } from 'vue';\nimport { use } from 'echarts/core';\nimport { CanvasRenderer } from 'echarts/renderers';\nimport { PieChart, LineChart } from 'echarts/charts';\nimport { GridComponent, TooltipComponent, TitleComponent, LegendComponent, TimelineComponent } from 'echarts/components';\nimport VChart from 'vue-echarts';\nimport axios from 'axios';\nimport QRCode from 'qrcode';\nimport io from 'socket.io-client'\nimport * as d3 from 'd3';\n\nuse([\n  CanvasRenderer,\n  PieChart,\n  LineChart,\n  GridComponent,\n  TooltipComponent,\n  TitleComponent,\n  LegendComponent,\n  TimelineComponent\n]);\n\nexport default {\n  name: 'StarSystemVisualization',\n  components: { VChart },\n  setup() {\n    // 状态数据\n    const totalAgv = ref(24);\n    const runningAgv = ref(18);\n    const chargingAgv = ref(4);\n    const maintenanceAgv = ref(2);\n    const agvList = ref([]);\n\n    // 图表配置\n    const pieOptions = ref({\n      title: {\n        text: 'AGV',\n        left: 'center',\n        top: 'center',\n        textStyle: {\n          color: '#fff',\n          fontSize: 16,\n          fontWeight: 'bold'\n        }\n      },\n      tooltip: {\n        trigger: 'item'\n      },\n      series: [{\n        type: 'pie',\n        radius: ['40%', '70%'],\n        avoidLabelOverlap: false,\n        label: {\n          show: true,\n          color: '#fff'\n        },\n        data: [\n          { value: 18, name: '运行中', itemStyle: { color: '#52c41a' } },\n          { value: 4, name: '充电中', itemStyle: { color: '#faad14' } },\n          { value: 2, name: '维修中', itemStyle: { color: '#ff4d4f' } }\n        ]\n      }]\n    });\n\n    const lineOptions = ref({\n      tooltip: {\n        trigger: 'axis',\n        backgroundColor: 'rgba(0,0,0,0.7)',\n        borderColor: 'rgba(255,255,255,0.5)',\n        textStyle: {\n          color: '#fff',\n          fontSize: 14\n        }\n      },\n      xAxis: {\n        type: 'category',\n        data: ['00:00', '04:00', '08:00', '12:00', '16:00', '20:00'],\n        axisLabel: { \n          color: '#fff',\n          fontSize: 14,\n          fontWeight: 'bold'\n        },\n        axisLine: {\n          lineStyle: {\n            color: 'rgba(255,255,255,0.5)',\n            width: 2\n          }\n        },\n        splitLine: {\n          show: true,\n          lineStyle: {\n            color: 'rgba(255,255,255,0.1)',\n            width: 1,\n            type: 'solid'\n          }\n        }\n      },\n      yAxis: {\n        type: 'value',\n        axisLabel: { \n          color: '#fff',\n          fontSize: 14,\n          fontWeight: 'bold'\n        },\n        axisLine: {\n          show: true,\n          lineStyle: {\n            color: 'rgba(255,255,255,0.5)',\n            width: 2\n          }\n        },\n        splitLine: {\n          show: true,\n          lineStyle: {\n            color: 'rgba(255,255,255,0.1)',\n            width: 1,\n            type: 'solid'\n          }\n        }\n      },\n      grid: {\n        top: '8%',\n        left: '3%',\n        right: '4%',\n        bottom: '8%',\n        containLabel: true\n      },\n      series: [{\n        data: [150, 230, 224, 218, 135, 147],\n        type: 'line',\n        smooth: true,\n        symbol: 'circle',\n        symbolSize: 12,\n        lineStyle: {\n          width: 5,\n          color: '#00b8d4',\n          shadowColor: 'rgba(0,184,212,0.5)',\n          shadowBlur: 10\n        },\n        itemStyle: {\n          color: '#ffffff',\n          borderWidth: 3,\n          borderColor: '#00b8d4'\n        },\n        emphasis: {\n          scale: true,\n          itemStyle: {\n            color: '#00b8d4',\n            borderColor: '#ffffff',\n            borderWidth: 4,\n            scale: 1.2\n          }\n        },\n        areaStyle: {\n          color: {\n            type: 'linear',\n            x: 0,\n            y: 0,\n            x2: 0,\n            y2: 1,\n            colorStops: [{\n              offset: 0,\n              color: 'rgba(0,184,212,0.3)'\n            }, {\n              offset: 1,\n              color: 'rgba(0,184,212,0.05)'\n            }]\n          }\n        }\n      }]\n    });\n\n    // 摄像头相关\n    const remoteVideo = ref(null);\n    const isConnected = ref(false);\n    const socket = ref(null);\n    const recognitionText = ref('');\n\n    // 方法\n    const getBatteryColor = (level) => {\n      if (level > 70) return '#52c41a';\n      if (level > 30) return '#faad14';\n      return '#ff4d4f';\n    };\n\n    const apiUrl = 'http://localhost:3000';\n\n    const fetchAgvData = async () => {\n      try {\n        const response = await axios.get(`${apiUrl}/api/agvs`);\n        agvList.value = response.data;\n        updateStatistics();\n      } catch (error) {\n        console.error('Error fetching AGV data:', error);\n      }\n    };\n\n    const updateAgvStatus = async (agv) => {\n      try {\n        await axios.put(`${apiUrl}/api/agvs/${agv.id}`, agv);\n        await fetchAgvData();\n      } catch (error) {\n        console.error('Error updating AGV status:', error);\n      }\n    };\n\n    const updateStatistics = () => {\n      if (agvList.value.length > 0) {\n        runningAgv.value = agvList.value.filter(agv => agv.task === '运输中').length;\n        chargingAgv.value = agvList.value.filter(agv => agv.task === '充电中').length;\n        maintenanceAgv.value = agvList.value.filter(agv => agv.task === '维修中').length;\n        totalAgv.value = agvList.value.length;\n      }\n    };\n\n    const connectToStream = () => {\n      if (!socket.value) {\n        const serverUrl = window.location.hostname === 'localhost' \n          ? 'http://localhost:3000' \n          : `https://${window.location.hostname}`;\n        \n        socket.value = io(serverUrl, {\n          transports: ['websocket', 'polling'],\n          path: '/socket.io'\n        });\n        \n        socket.value.on('connect', () => {\n          console.log('Connected to server');\n          isConnected.value = true;\n        });\n\n        socket.value.on('videoFrame', (data) => {\n          if (remoteVideo.value) {\n            remoteVideo.value.src = data.image;\n            recognitionText.value = data.recognitionText;\n          }\n        });\n\n        socket.value.on('connect_error', (error) => {\n          console.error('Connection error:', error);\n          isConnected.value = false;\n          socket.value = null;\n        });\n\n        socket.value.on('disconnect', () => {\n          console.log('Disconnected from server');\n          isConnected.value = false;\n          if (remoteVideo.value) {\n            remoteVideo.value.src = '';\n          }\n          recognitionText.value = '';\n          socket.value = null;\n        });\n      }\n    };\n\n    const disconnectFromStream = () => {\n      if (socket.value) {\n        socket.value.disconnect();\n        socket.value = null;\n        isConnected.value = false;\n        if (remoteVideo.value) {\n          remoteVideo.value.src = '';\n        }\n        recognitionText.value = '';\n      }\n    };\n\n    const qrcodeRef = ref(null);\n    \n    const chartRef = ref(null);\n\n    // 星图绘制相关\n    const selectedConstellation = ref(null);\n    const selectedSeason = ref('春');\n    const constellationList = ref([\n      {\n        id: 1,\n        name: '角宿',\n        mansion: '东宫苍龙',\n        starCount: 12,\n        geography: '齐地',\n        history: '《史记·天官书》：角，天门，郊祀前望。'\n      },\n      {\n        id: 2,\n        name: '亢宿',\n        mansion: '东宫苍龙',\n        starCount: 4,\n        geography: '宋地',\n        history: '《史记·天官书》：亢，阙庭，人主奉邑。'\n      },\n      // ... 其他星官数据\n    ]);\n\n    // 时间轴配置\n    const timelineOptions = ref({\n      baseOption: {\n        timeline: {\n          axisType: 'category',\n          autoPlay: true,\n          playInterval: 3000,\n          data: ['商周', '春秋战国', '秦汉', '魏晋南北朝', '隋唐', '宋元', '明清'],\n          label: {\n            color: '#fff'\n          }\n        },\n        title: {\n          text: '中国古代天文仪器发展史',\n          textStyle: {\n            color: '#fff',\n            fontSize: 16\n          }\n        },\n        tooltip: {\n          trigger: 'axis',\n          backgroundColor: 'rgba(0,0,0,0.7)',\n          borderColor: 'rgba(255,255,255,0.5)',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        xAxis: {\n          type: 'category',\n          data: ['圭表', '浑天仪', '简仪', '漏刻', '浑象', '圭表'],\n          axisLabel: { \n            color: '#fff',\n            fontSize: 14\n          }\n        },\n        yAxis: {\n          type: 'value',\n          axisLabel: { \n            color: '#fff',\n            fontSize: 14\n          }\n        },\n        series: [{\n          type: 'line',\n          data: [10, 25, 40, 55, 70, 85],\n          lineStyle: {\n            width: 5,\n            color: '#00b8d4'\n          },\n          symbolSize: 12\n        }]\n      }\n    });\n\n    // 星图绘制相关\n    const starMapCanvas = ref(null);\n    let starMap = null;\n\n    const initStarMap = () => {\n      if (!starMapCanvas.value) return;\n\n      const width = starMapCanvas.value.parentElement.clientWidth;\n      const height = starMapCanvas.value.parentElement.clientHeight;\n\n      starMap = d3.select(starMapCanvas.value)\n        .attr('width', width)\n        .attr('height', height);\n\n      // 在这里添加星图绘制逻辑\n    };\n\n    const selectSeason = (season) => {\n      selectedSeason.value = season;\n      // 更新星图显示\n    };\n\n    const zoomIn = () => {\n      // 实现放大功能\n    };\n\n    const zoomOut = () => {\n      // 实现缩小功能\n    };\n\n    const resetView = () => {\n      // 重置视图\n    };\n\n    onMounted(async () => {\n      await nextTick();\n      await fetchAgvData();\n      const refreshInterval = setInterval(fetchAgvData, 1500);\n\n      // 生成二维码\n      if (qrcodeRef.value) {\n        const localIP = '172.20.10.3'; // 使用你的实际IP地址\n        const mobileUrl = `http://${localIP}:8080/#/mobile-camera`;\n        try {\n          await QRCode.toCanvas(qrcodeRef.value, mobileUrl, {\n            width: 130,\n            margin: 0,\n            color: {\n              dark: '#FFFFFF',\n              light: '#00000000'\n            }\n          });\n        } catch (err) {\n          console.error('Error generating QR code:', err);\n        }\n      }\n\n      initStarMap();\n      \n      const handleResize = () => {\n        if (starMapCanvas.value) {\n          initStarMap();\n        }\n      };\n\n      window.addEventListener('resize', handleResize);\n      \n      onUnmounted(() => {\n        clearInterval(refreshInterval);\n        disconnectFromStream();\n        window.removeEventListener('resize', handleResize);\n      });\n    });\n\n    return {\n      totalAgv,\n      runningAgv,\n      chargingAgv,\n      maintenanceAgv,\n      agvList,\n      pieOptions,\n      lineOptions,\n      remoteVideo,\n      isConnected,\n      connectToStream,\n      disconnectFromStream,\n      qrcodeRef,\n      chartRef,\n      getBatteryColor,\n      recognitionText,\n      selectedConstellation,\n      selectedSeason,\n      constellationList,\n      timelineOptions,\n      starMapCanvas,\n      selectSeason,\n      zoomIn,\n      zoomOut,\n      resetView\n    };\n  }\n};\n</script>\n\n<style scoped>\n.content {\n  padding: 20px;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n  color: #fff;\n  height: calc(97vh - 60px);\n  border: 1px solid #4a90e2;\n  border-radius: 12px;\n  margin: 15px 20px;\n  background: linear-gradient(to bottom, rgba(13, 15, 44, 0.95), rgba(13, 15, 44, 0.85));\n  box-shadow: 0 0 20px rgba(74, 144, 226, 0.3);\n}\n\n.status-cards {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  gap: 20px;\n}\n\n.status-card {\n  background: rgba(13, 15, 44, 0.7);\n  border-radius: 8px;\n  padding: 30px;\n  text-align: center;\n  border: 1px solid rgba(74, 144, 226, 0.6);\n  transition: all 0.3s ease;\n  height: 160px;\n  backdrop-filter: blur(10px);\n}\n\n.status-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 5px 15px rgba(74, 144, 226, 0.4);\n}\n\n.status-card h3 {\n  margin: 0;\n  font-size: 18px;\n  color: rgba(255, 255, 255, 0.9);\n  font-weight: 500;\n}\n\n.real {\n  color: #4a90e2;\n}\n\n.value {\n  font-size: 32px;\n  font-weight: bold;\n  margin-top: 15px;\n  background: linear-gradient(45deg, #4a90e2, #64b5f6);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\n\n.charts-container {\n  display: grid;\n  grid-template-columns: repeat(2, 1fr);\n  gap: 20px;\n}\n\n.chart-box {\n  background: rgba(13, 15, 44, 0.7);\n  border-radius: 8px;\n  padding: 20px;\n  border: 1px solid rgba(74, 144, 226, 0.6);\n  height: 450px;\n  backdrop-filter: blur(10px);\n}\n\n.constellation-content {\n  display: flex;\n  gap: 20px;\n  height: 380px;\n}\n\n.star-map-container {\n  flex: 1;\n  background: rgba(0, 0, 0, 0.5);\n  border-radius: 8px;\n  overflow: hidden;\n  position: relative;\n}\n\n.star-map {\n  width: 100%;\n  height: 100%;\n}\n\n.constellation-info {\n  position: absolute;\n  bottom: 20px;\n  left: 20px;\n  right: 20px;\n  background: rgba(13, 15, 44, 0.9);\n  color: white;\n  padding: 15px;\n  border-radius: 4px;\n  font-size: 14px;\n  line-height: 1.6;\n  border: 1px solid rgba(74, 144, 226, 0.4);\n}\n\n.navigation-controls {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  display: flex;\n  gap: 10px;\n}\n\n.control-panel {\n  width: 150px;\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.season-filter {\n  display: flex;\n  flex-direction: column;\n  gap: 10px;\n}\n\n.season-filter button {\n  width: 100%;\n  padding: 12px;\n  background: rgba(74, 144, 226, 0.2);\n  color: white;\n  border: 1px solid rgba(74, 144, 226, 0.4);\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.season-filter button.active {\n  background: rgba(74, 144, 226, 0.8);\n  border-color: #4a90e2;\n}\n\n.season-filter button:hover:not(.active) {\n  background: rgba(74, 144, 226, 0.4);\n}\n\n.constellation-table {\n  background: rgba(13, 15, 44, 0.7);\n  border-radius: 8px;\n  padding: 20px;\n  border: 1px solid rgba(74, 144, 226, 0.6);\n  backdrop-filter: blur(10px);\n}\n\n.constellation-table h3 {\n  margin: 0 0 20px 0;\n  font-size: 18px;\n  text-align: center;\n  color: #fff;\n}\n\ntable {\n  width: 100%;\n  border-collapse: separate;\n  border-spacing: 0;\n}\n\nth, td {\n  padding: 12px;\n  text-align: left;\n  border-bottom: 1px solid rgba(74, 144, 226, 0.2);\n  color: #fff;\n}\n\nth {\n  font-weight: 500;\n  color: rgba(255, 255, 255, 0.9);\n  background: rgba(74, 144, 226, 0.1);\n}\n\n.history-text {\n  max-width: 300px;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n.chart {\n  width: 100%;\n  height: 100%;\n}\n\nbutton {\n  font-family: \"Microsoft YaHei\", sans-serif;\n}\n\nbutton:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n/* 添加一些星空元素的动画效果 */\n@keyframes twinkle {\n  0% { opacity: 0.3; }\n  50% { opacity: 1; }\n  100% { opacity: 0.3; }\n}\n\n.star-map::before {\n  content: '';\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  background-image: radial-gradient(circle at center, rgba(255,255,255,0.1) 0%, transparent 1%);\n  background-size: 15px 15px;\n  animation: twinkle 3s infinite;\n}\n\n.nav-header {\n  height: 60px;\n  padding: 40px 20px;\n  display: flex;\n  align-items: center;\n  gap: 30px;\n  background: rgba(13, 15, 44, 0.6);\n  position: relative;\n  border: 1px solid rgba(0, 195, 255, 0.3);\n}\n\n.nav-header::before,\n.nav-header::after {\n  content: '';\n  position: absolute;\n  width: 20px;\n  height: 20px;\n}\n\n.nav-header::before {\n  top: -1px;\n  left: -1px;\n  border-top: 2px solid #00c3ff;\n  border-left: 2px solid #00c3ff;\n  box-shadow: -2px -2px 10px rgba(0, 195, 255, 0.5);\n}\n\n.nav-header::after {\n  bottom: -1px;\n  right: -1px;\n  border-bottom: 2px solid #00c3ff;\n  border-right: 2px solid #00c3ff;\n  box-shadow: 2px 2px 10px rgba(0, 195, 255, 0.5);\n}\n\n.nav-text {\n  color: rgba(255, 255, 255, 0.8);\n  text-decoration: none;\n  font-size: 16px;\n  transition: all 0.3s ease;\n  position: relative;\n  padding: 5px 15px;\n}\n\n.nav-text:hover {\n  color: #00c3ff;\n  text-shadow: 0 0 10px rgba(0, 195, 255, 0.5);\n}\n\n.nav-text.active {\n  color: #00c3ff;\n  background: rgba(0, 195, 255, 0.1);\n  border: 1px solid rgba(0, 195, 255, 0.3);\n}\n</style>\n"]}]}