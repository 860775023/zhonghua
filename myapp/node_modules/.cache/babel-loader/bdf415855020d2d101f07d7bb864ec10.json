mFive,ItemSix,ItemSeven\n  },\n\n  setup(){\n    let $echarts=inject(\"echarts\")\n    let $http=inject(\"axios\")\n    console.log($echarts)\n    console.log($http)\n  }\n};\n</script>\n\n<style lang=\"less\">\nheader {\n  height: 1rem;\n  width: 100%;\n  background-color: rgba(0, 0, 255, 0.2);\n  display: flex;\n  align-items: center;\n  padding: 0 1rem;\n  \n  div:first-child {\n    display: flex;\n    gap: 30px;\n    font-size: 15px;\n  }\n\n \n}\n\n@keyframes borderGlow {\n  0%, 100% {\n    opacity: 0.5;\n    filter: drop-shadow(0 0 5px rgba(0, 195, 255, 0.3));\n  }\n  50% {\n    opacity: 0.8;\n    filter: drop-shadow(0 0 15px rgba(0, 195, 255, 0.5));\n  }\n}\n\n// 添加扫描线动画\n@keyframes scanline {\n  0% {\n    transform: translateY(-100%);\n  }\n  100% {\n    transform: translateY(100%);\n  }\n}\n\n// 添加光晕动画\n@keyframes glow {\n  0%, 100% {\n    box-shadow: 0 0 5px rgba(0, 195, 255, 0.3),\n                0 0 10px rgba(0, 195, 255, 0.2),\n                0 0 15px rgba(0, 195, 255, 0.1);\n  }\n  50% {\n    box-shadow: 0 0 10px rgba(0, 195, 255, 0.4),\n                0 0 20px rgba(0, 195, 255, 0.3),\n                0 0 30px rgba(0, 195, 255, 0.2);\n  }\n}\n\n.nav-text {\n  color: hsl(120, 4%, 95%);\n  text-decoration: none;\n  cursor: pointer;\n  \n  &:hover {\n    color: #1890ff;\n  }\n}\n\n// 大容器的样式\n.container {\n  min-width: 1200px;\n  max-width: 2048px;\n  margin: 0 auto;\n  padding: 0.125rem 0.125rem 0;\n  \n  .tech-border {\n    position: relative;\n    width: 100%;\n    height: 100%;\n    display: flex;\n    padding: 20px;\n    \n    \n    // 四个角的装饰\n    .corner-lt,\n    .corner-rt,\n    .corner-lb,\n    .corner-rb {\n      position: absolute;\n      width: 20px;\n      height: 20px;\n      \n      &:before,\n      &:after {\n        content: '';\n        position: absolute;\n        background: #00c3ff;\n      }\n    }\n    \n    // 左上角\n    .corner-lt {\n      top: 0;\n      left: 0;\n      \n      &:before {\n        top: 0;\n        left: 0;\n        width: 2px;\n        height: 40px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n      \n      &:after {\n        top: 0;\n        left: 0;\n        width: 40px;\n        height: 2px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n    }\n    \n    // 右上角\n    .corner-rt {\n      top: 0;\n      right: 0;\n      \n      &:before {\n        top: 0;\n        right: 0;\n        width: 2px;\n        height: 40px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n      \n      &:after {\n        top: 0;\n        right: 0;\n        width: 40px;\n        height: 2px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n    }\n    \n    // 左下角\n    .corner-lb {\n      bottom: 0;\n      left: 0;\n      \n      &:before {\n        bottom: 0;\n        left: 0;\n        width: 2px;\n        height: 40px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n      \n      &:after {\n        bottom: 0;\n        left: 0;\n        width: 40px;\n        height: 2px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n    }\n    \n    // 右下角\n    .corner-rb {\n      bottom: 0;\n      right: 0;\n      \n      &:before {\n        bottom: 0;\n        right: 0;\n        width: 2px;\n        height: 40px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n      \n      &:after {\n        bottom: 0;\n        right: 0;\n        width: 40px;\n        height: 2px;\n        box-shadow: 0 0 10px #00c3ff;\n      }\n    }\n    \n    // 添加边框动画\n    &:before {\n      content: '';\n      position: absolute;\n      top: 10px;\n      left: 10px;\n      right: 10px;\n      bottom: 10px;\n      border: 1px solid rgba(0,195,255,0.3);\n      animation: borderGlow 2s infinite;\n    }\n  }\n\n  // 保持原有的布局样式\n  .itemLeft,\n  .itemRight {\n    flex: 3;\n  }\n\n  .itemCenter {\n    flex: 5;\n    height: 567px;\n    padding: 0.125rem;\n    margin: .25rem;\n  }\n\n  .y{\n     // 确保宽度与.itemCenter一致\n  position: absolute; \n  width: 780px;\n    border: 1px solid blue;\n    background-color: rgba(12,130,255,.85);\n   height: 273px;\n   left: 463px; \n  }\n\n}\n\n.nav-menu {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin: 20px 0;\n}\n\n.nav-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 15px 25px;\n  background: rgba(25, 118, 210, 0.1);\n  border-radius: 8px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  text-decoration: none; /* 移除下划线 */\n}\n\n.nav-item:hover {\n  background: rgba(25, 118, 210, 0.2);\n  transform: translateY(-2px);\n}\n\n.nav-item i {\n  font-size: 24px;\n  margin-bottom: 8px;\n  color: #1890ff;\n}\n\n.nav-item span {\n  color: #fff;\n  font-size: 16px;\n}\n\n/* 激活状态的样式 */\n.router-link-active {\n  background: rgba(25, 118, 210, 0.3);\n}\n\n// 边框发光动画\n@keyframes borderGlow {\n  0% {\n    box-shadow: 0 0 5px rgba(0,195,255,0.3) inset;\n  }\n  50% {\n    box-shadow: 0 0 20px rgba(0,195,255,0.5) inset;\n  }\n  100% {\n    box-shadow: 0 0 5px rgba(0,195,255,0.3) inset;\n  }\n}\n</style>"],"sourceRoot":""}]}