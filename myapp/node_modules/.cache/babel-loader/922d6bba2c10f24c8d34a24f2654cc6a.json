"content\">\r\n      <div class=\"charts-grid\">\r\n        <!-- 学者著作关系图 -->\r\n        <div class=\"chart-box relationship-chart\">\r\n          <div class=\"chart-title\">天文学术传承谱系</div>\r\n          <div class=\"legend\">\r\n            <div class=\"legend-item\">\r\n              <span class=\"dot scholar\"></span>学者\r\n            </div>\r\n            <div class=\"legend-item\">\r\n              <span class=\"dot book\"></span>著作\r\n            </div>\r\n            <div class=\"legend-item\">\r\n              <span class=\"line\"></span>撰写/传承关系\r\n            </div>\r\n          </div>\r\n          <vue-echarts \r\n            ref=\"relationshipChart\"\r\n            :option=\"relationshipOptions\" \r\n            class=\"chart\" \r\n            :autoresize=\"true\"\r\n          />\r\n        </div>\r\n\r\n        <!-- 学术流派分布 -->\r\n        <div class=\"chart-box\">\r\n          <div class=\"chart-title\">学术流派分布</div>\r\n          <vue-echarts \r\n            ref=\"schoolChart\"\r\n            :option=\"schoolOptions\" \r\n            class=\"chart\" \r\n            :autoresize=\"true\"\r\n          />\r\n        </div>\r\n\r\n        <!-- 重要著作年代分布 -->\r\n        <div class=\"chart-box\">\r\n          <div class=\"chart-title\">重要天文著作年代分布</div>\r\n          <vue-echarts \r\n            ref=\"timelineChart\"\r\n            :option=\"timelineOptions\" \r\n            class=\"chart\" \r\n            :autoresize=\"true\"\r\n          />\r\n        </div>\r\n\r\n        <!-- 学术影响力分析 -->\r\n        \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.analysis-container {\r\n  padding: 0;\r\n  color: #fff;\r\n  height: 98vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.nav-header {\r\n  height: 60px;\r\n  padding: 40px 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 30px;\r\n  background: rgba(13, 15, 44, 0.6);\r\n  position: relative;\r\n  border: 1px solid rgba(0, 195, 255, 0.3);\r\n}\r\n\r\n.nav-header: