A,cAAA,WAAW,CAAC,YAAM;AAChB,gBAAA,aAAa,CAAC,eAAD,CAAb;AACA,gBAAA,UAAU;AACX,eAHU,CAAX;;AApBQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAD,GAAT;AA0BA,WAAO;AACL,MAAA,QAAQ,EAAR,QADK;AAEL,MAAA,UAAU,EAAV,UAFK;AAGL,MAAA,WAAW,EAAX,WAHK;AAIL,MAAA,cAAc,EAAd,cAJK;AAKL,MAAA,OAAO,EAAP,OALK;AAML,MAAA,UAAU,EAAV,UANK;AAOL,MAAA,WAAW,EAAX,WAPK;AAQL,MAAA,YAAY,EAAZ,YARK;AASL,MAAA,cAAc,EAAd,cATK;AAUL,MAAA,MAAM,EAAN,MAVK;AAWL,MAAA,eAAe,EAAf,eAXK;AAYL,MAAA,WAAW,EAAX,WAZK;AAaL,MAAA,UAAS,EAAT;AAbK,KAAP;AAeF;AAnQa,CAAf","sourcesContent":["<template>\r\n  <div class=\"monitor-container\">\r\n    <div class=\"nav-header\">\r\n      <router-link to=\"/\" class=\"nav-text\">仓储实况</router-link>\r\n      <span class=\"nav-text active\">实时监测</span>\r\n      <router-link to=\"/analysis\" class=\"nav-text\">数据分析</router-link>\r\n    </div>\r\n    <div class=\"content\">\r\n      <!-- 顶部状态卡片 -->\r\n      <div class=\"status-cards\">\r\n        <div class=\"status-card\">\r\n          <h3>AGV总数</h3>\r\n          <div class=\"value\">{{ totalAgv }}</div>\r\n        </div>\r\n        <div class=\"status-card\">\r\n          <h3>运行中</h3>\r\n          <div class=\"value\">{{ runningAgv }}</div>\r\n        </div>\r\n        <div class=\"status-card\">\r\n          <h3>充电中</h3>\r\n          <div class=\"value\">{{ chargingAgv }}</div>\r\n        </div>\r\n        <div class=\"status-card\">\r\n          <h3>维修中</h3>\r\n          <div class=\"value\">{{ maintenanceAgv }}</div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 图表区域 -->\r\n      <div class=\"charts-container\">\r\n        <div class=\"chart-box camera-box\">\r\n          <h3>AGV实时监控</h3>\r\n          <div class=\"monitor-content\">\r\n            <div class=\"main-area\">\r\n              <!-- 摄像头区域 -->\r\n              <div class=\"camera-view\">\r\n                <video ref=\"videoElement\" autoplay playsinline></video>\r\n              </div>\r\n              <!-- 控制按钮和二维码区域 -->\r\n              <div class=\"control-area\">\r\n                <div class=\"camera-controls\">\r\n                  <button @click=\"startCamera\" :disabled=\"isCameraActive\">开启摄像头</button>\r\n                  <button @click=\"stopCamera\" :disabled=\"!isCameraActive\">关闭摄像头</button>\r\n                </div>\r\n                <div class=\"qr-code\">\r\n                  <h4>手机扫码访问</h4>\r\n                  <canvas ref=\"qrcode\"></canvas>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"chart-box\">\r\n          <h3>AGV任务完成趋势</h3>\r\n          <v-chart :option=\"lineOptions\" autoresize />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- AGV状态表格 -->\r\n      <div class=\"table-container\">\r\n        <h3>AGV实时状态</h3>\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>AGV编号</th>\r\n              <th>当前位置</th>\r\n              <th>任务状态</th>\r\n              <th>电量</th>\r\n              <th>运行状态</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"item in agvList\" :key=\"item.id\">\r\n              <td>{{ item.id }}</td>\r\n              <td>{{ item.location }}</td>\r\n              <td>{{ item.task }}</td>\r\n              <td>\r\n                <div class=\"battery-status\">\r\n                  <div class=\"battery-bar\" :style=\"{ width: item.battery + '%', background: getBatteryColor(item.battery) }\"></div>\r\n                  <span>{{ item.battery }}%</span>\r\n                </div>\r\n              </td>\r\n              <td>\r\n                <span :class=\"['status-indicator', item.status]\"></span>\r\n                {{ item.statusText }}\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, onUnmounted } from 'vue';\r\nimport { use } from 'echarts/core';\r\nimport { CanvasRenderer } from 'echarts/renderers';\r\nimport { PieChart, LineChart } from 'echarts/charts';\r\nimport { GridComponent, TooltipComponent, TitleComponent, LegendComponent } from 'echarts/components';\r\nimport VChart from 'vue-echarts';\r\nimport axios from 'axios';\r\nimport QRCode from 'qrcode';\r\n\r\nuse([\r\n  CanvasRenderer,\r\n  PieChart,\r\n  LineChart,\r\n  GridComponent,\r\n  TooltipComponent,\r\n  TitleComponent,\r\n  LegendComponent\r\n]);\r\n\r\nexport default {\r\n  name: 'RealTimeMonitor',\r\n  components: { VChart },\r\n  setup() {\r\n    // 状态数据\r\n    const totalAgv = ref(24);\r\n    const runningAgv = ref(18);\r\n    const chargingAgv = ref(4);\r\n    const maintenanceAgv = ref(2);\r\n    const agvList = ref([]);\r\n\r\n    // 图表配置\r\n    const pieOptions = ref({\r\n      title: {\r\n        text: 'AGV',\r\n        left: 'center',\r\n        top: 'center',\r\n        textStyle: {\r\n          color: '#fff',\r\n          fontSize: 16,\r\n          fontWeight: 'bold'\r\n        }\r\n      },\r\n      tooltip: {\r\n        trigger: 'item'\r\n      },\r\n      series: [{\r\n        type: 'pie',\r\n        radius: ['40%', '70%'],\r\n        avoidLabelOverlap: false,\r\n        label: {\r\n          show: true,\r\n          color: '#fff'\r\n        },\r\n        data: [\r\n          { value: 18, name: '运行中', itemStyle: { color: '#52c41a' } },\r\n          { value: 4, name: '充电中', itemStyle: { color: '#faad14' } },\r\n          { value: 2, name: '维修中', itemStyle: { color: '#ff4d4f' } }\r\n        ]\r\n      }]\r\n    });\r\n\r\n    const lineOptions = ref({\r\n      tooltip: {\r\n        trigger: 'axis',\r\n        backgroundColor: 'rgba(0,0,0,0.7)',\r\n        borderColor: 'rgba(255,255,255,0.5)',\r\n        textStyle: {\r\n          color: '#fff',\r\n          fontSize: 14\r\n        }\r\n      },\r\n      xAxis: {\r\n        type: 'category',\r\n        data: ['00:00', '04:00', '08:00', '12:00', '16:00', '20:00'],\r\n        axisLabel: { \r\n          color: 'rgba(255,255,255,0.8)',\r\n          fontSize: 14,\r\n          fontWeight: 'bold'\r\n        },\r\n        axisLine: {\r\n          lineStyle: {\r\n            color: 'rgba(255,255,255,0.5)',\r\n            width: 2\r\n          }\r\n        },\r\n        splitLine: {\r\n          show: true,\r\n          lineStyle: {\r\n            color: 'rgba(255,255,255,0.1)',\r\n            width: 1,\r\n            type: 'solid'\r\n          }\r\n        }\r\n      },\r\n      yAxis: {\r\n        type: 'value',\r\n        axisLabel: { \r\n          color: 'rgba(255,255,255,0.8)',\r\n          fontSize: 14,\r\n          fontWeight: 'bold'\r\n        },\r\n        axisLine: {\r\n          show: true,\r\n          lineStyle: {\r\n            color: 'rgba(255,255,255,0.5)',\r\n            width: 2\r\n          }\r\n        },\r\n        splitLine: {\r\n          show: true,\r\n          lineStyle: {\r\n            color: 'rgba(255,255,255,0.1)',\r\n            width: 1,\r\n            type: 'solid'\r\n          }\r\n        }\r\n      },\r\n      grid: {\r\n        top: '8%',\r\n        left: '3%',\r\n        right: '4%',\r\n        bottom: '8%',\r\n        containLabel: true\r\n      },\r\n      series: [{\r\n        data: [150, 230, 224, 218, 135, 147],\r\n        type: 'line',\r\n        smooth: true,\r\n        symbol: 'circle',\r\n        symbolSize: 12,\r\n        lineStyle: {\r\n          width: 5,\r\n          color: '#00b8d4',\r\n          shadowColor: 'rgba(0,184,212,0.5)',\r\n          shadowBlur: 10\r\n        },\r\n        itemStyle: {\r\n          color: '#ffffff',\r\n          borderWidth: 3,\r\n          borderColor: '#00b8d4'\r\n        },\r\n        emphasis: {\r\n          scale: true,\r\n          itemStyle: {\r\n            color: '#00b8d4',\r\n            borderColor: '#ffffff',\r\n            borderWidth: 4,\r\n            scale: 1.2\r\n          }\r\n        },\r\n        areaStyle: {\r\n          color: {\r\n            type: 'linear',\r\n            x: 0,\r\n            y: 0,\r\n            x2: 0,\r\n            y2: 1,\r\n            colorStops: [{\r\n              offset: 0,\r\n              color: 'rgba(0,184,212,0.3)'\r\n            }, {\r\n              offset: 1,\r\n              color: 'rgba(0,184,212,0.05)'\r\n            }]\r\n          }\r\n        }\r\n      }]\r\n    });\r\n\r\n    // 摄像头相关\r\n    const videoElement = ref(null);\r\n    const isCameraActive = ref(false);\r\n    const qrcode = ref(null);\r\n    let mediaStream = null;\r\n\r\n    // 方法\r\n    const getBatteryColor = (level) => {\r\n      if (level > 70) return '#52c41a';\r\n      if (level > 30) return '#faad14';\r\n      return '#ff4d4f';\r\n    };\r\n\r\n    const fetchAgvData = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:5000/api/agvs');\r\n        agvList.value = response.data;\r\n        updateStatistics();\r\n      } catch (error) {\r\n        console.error('Error fetching AGV data:', error);\r\n      }\r\n    };\r\n\r\n    const updateAgvStatus = async (agv) => {\r\n      try {\r\n        await axios.put(`http://localhost:5000/api/agvs/${agv.id}`, agv);\r\n        await fetchAgvData();\r\n      } catch (error) {\r\n        console.error('Error updating AGV status:', error);\r\n      }\r\n    };\r\n\r\n    const updateStatistics = () => {\r\n      if (agvList.value.length > 0) {\r\n        runningAgv.value = agvList.value.filter(agv => agv.task === '运输中').length;\r\n        chargingAgv.value = agvList.value.filter(agv => agv.task === '充电中').length;\r\n        maintenanceAgv.value = agvList.value.filter(agv => agv.task === '维修中').length;\r\n        totalAgv.value = agvList.value.length;\r\n      }\r\n    };\r\n\r\n    const startCamera = async () => {\r\n      try {\r\n        mediaStream = await navigator.mediaDevices.getUserMedia({\r\n          video: { facingMode: 'environment' }\r\n        });\r\n        \r\n        if (videoElement.value) {\r\n          videoElement.value.srcObject = mediaStream;\r\n          isCameraActive.value = true;\r\n        }\r\n      } catch (error) {\r\n        console.error('Error accessing camera:', error);\r\n        alert('无法访问摄像头，请确保已授予权限并且设备已连接。');\r\n      }\r\n    };\r\n\r\n    const stopCamera = () => {\r\n      if (mediaStream) {\r\n        mediaStream.getTracks().forEach(track => track.stop());\r\n        if (videoElement.value) {\r\n          videoElement.value.srcObject = null;\r\n        }\r\n        isCameraActive.value = false;\r\n      }\r\n    };\r\n\r\n    // 生命周期钩子\r\n    onMounted(async () => {\r\n      await fetchAgvData();\r\n      const refreshInterval = setInterval(fetchAgvData, 1500);\r\n      \r\n      // 生成移动端页面的URL\r\n      const mobileUrl = `${window.location.origin}/mobile-camera`;\r\n      \r\n      if (qrcode.value) {\r\n        QRCode.toCanvas(qrcode.value, mobileUrl, {\r\n          width: 128,\r\n          margin: 0,\r\n          color: {\r\n            dark: '#FFFFFF',\r\n            light: '#00000000'\r\n          }\r\n        }).catch(err => {\r\n          console.error('Error generating QR code:', err);\r\n        });\r\n      }\r\n\r\n      onUnmounted(() => {\r\n        clearInterval(refreshInterval);\r\n        stopCamera();\r\n      });\r\n    });\r\n\r\n    return {\r\n      totalAgv,\r\n      runningAgv,\r\n      chargingAgv,\r\n      maintenanceAgv,\r\n      agvList,\r\n      pieOptions,\r\n      lineOptions,\r\n      videoElement,\r\n      isCameraActive,\r\n      qrcode,\r\n      getBatteryColor,\r\n      startCamera,\r\n      stopCamera\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.monitor-container {\r\n  padding: 0;\r\n  color: #fff;\r\n}\r\n\r\n.nav-header {\r\n  height: 1rem;\r\n  width: 100%;\r\n  background-color: rgba(0, 0, 255, 0.2);\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 1rem;\r\n  gap: 30px;\r\n  margin-bottom: 20px;\r\n  margin-top: 0;\r\n}\r\n\r\n.content {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n  padding: 0 20px;\r\n}\r\n\r\n.status-cards,\r\n.charts-container,\r\n.table-container {\r\n  background: rgba(12,130,255,.85);\r\n  position: relative;\r\n  border-radius: 12px;\r\n  border: 1px solid rgba(0, 229, 255, 0.6);\r\n}\r\n\r\n.status-cards::before,\r\n.charts-container::before,\r\n.table-container::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: -1px;\r\n  left: -1px;\r\n  right: -1px;\r\n  bottom: -1px;\r\n  border-radius: 12px;\r\n  border: 1px solid rgba(0, 229, 255, 0.8);\r\n  clip-path: polygon(\r\n    0% 15%, 0% 0%, 15% 0%,\r\n    85% 0%, 100% 0%, 100% 15%,\r\n    100% 85%, 100% 100%, 85% 100%,\r\n    15% 100%, 0% 100%, 0% 85%\r\n  );\r\n}\r\n\r\n.status-cards::after,\r\n.charts-container::after,\r\n.table-container::after {\r\n  content: '';\r\n  position: absolute;\r\n  inset: 0;\r\n  border-radius: 12px;\r\n  padding: 1px;\r\n  background: linear-gradient(\r\n    135deg,\r\n    rgba(0, 229, 255, 0.5) 0%,\r\n    transparent 50%,\r\n    rgba(0, 229, 255, 0.5) 100%\r\n  );\r\n  mask: \r\n    linear-gradient(#fff 0 0) content-box, \r\n    linear-gradient(#fff 0 0);\r\n  -webkit-mask: \r\n    linear-gradient(#fff 0 0) content-box, \r\n    linear-gradient(#fff 0 0);\r\n  mask-composite: exclude;\r\n  -webkit-mask-composite: xor;\r\n  pointer-events: none;\r\n}\r\n\r\n.status-cards {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  gap: 20px;\r\n  padding: 20px;\r\n}\r\n\r\n.charts-container {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 20px;\r\n  min-height: 400px;\r\n  padding: 20px;\r\n}\r\n\r\n.table-container {\r\n  padding: 20px;\r\n}\r\n\r\n.status-card {\r\n  background: rgba(12,130,255,.85);\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n  border: 1px solid blue;\r\n}\r\n\r\n.status-card .value {\r\n  font-size: 32px;\r\n  font-weight: bold;\r\n  margin-top: 10px;\r\n}\r\n\r\n.chart-box {\r\n  background: rgba(12,130,255,.85);\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  height: 400px;\r\n  position: relative;\r\n  border: 1px solid blue;\r\n}\r\n\r\n.chart-box h3 {\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.chart {\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: separate;\r\n  border-spacing: 0;\r\n  margin-top: 20px;\r\n}\r\n\r\nth, td {\r\n  padding: 12px;\r\n  text-align: left;\r\n  border-bottom: 1px solid rgba(244, 246, 240, 0.393);\r\n}\r\n\r\n.battery-status {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.battery-bar {\r\n  height: 8px;\r\n  border-radius: 4px;\r\n  min-width: 100px;\r\n}\r\n\r\n.status-indicator {\r\n  display: inline-block;\r\n  width: 8px;\r\n  height: 8px;\r\n  border-radius: 50%;\r\n  margin-right: 8px;\r\n}\r\n\r\n.status-indicator.running {\r\n  background: #52c41a;\r\n}\r\n\r\n.status-indicator.charging {\r\n  background: #faad14;\r\n}\r\n\r\n.status-indicator.maintenance {\r\n  background: #ff4d4f;\r\n}\r\n\r\n.nav-text {\r\n  color: hsl(120, 4%, 95%);\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n  font-size: 15px;\r\n}\r\n\r\n.nav-text:hover {\r\n  color: #1890ff;\r\n}\r\n\r\n.active {\r\n  color: #1890ff;\r\n}\r\n\r\n/* 修改摄像头和二维码相关样式 */\r\n.monitor-content {\r\n  height: calc(100% - 60px);\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.main-area {\r\n  display: flex;\r\n  height: 100%;\r\n  gap: 20px;\r\n}\r\n\r\n.camera-view {\r\n  flex: 1;\r\n  height: 100%;\r\n  background: #000;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r\n\r\nvideo {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.control-area {\r\n  width: 150px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  gap: 20px;\r\n}\r\n\r\n.camera-controls {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n}\r\n\r\n.camera-controls button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n  background: rgba(24, 144, 255, 0.8);\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background 0.3s;\r\n}\r\n\r\n.camera-controls button:hover:not(:disabled) {\r\n  background: rgba(24, 144, 255, 1);\r\n}\r\n\r\n.camera-controls button:disabled {\r\n  background: rgba(0, 0, 0, 0.25);\r\n  cursor: not-allowed;\r\n}\r\n\r\n.qr-code {\r\n  background: rgba(0, 0, 0, 0.2);\r\n  padding: 10px;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n}\r\n\r\n.qr-code h4 {\r\n  margin: 0 0 10px 0;\r\n  font-size: 14px;\r\n  color: rgba(255, 255, 255, 0.8);\r\n}\r\n\r\n.qr-code canvas {\r\n  width: 128px;\r\n  height: 128px;\r\n  background: transparent;\r\n}\r\n\r\n/* 响应式布局调整 */\r\n@media (max-width: 1200px) {\r\n  .main-area {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .control-area {\r\n    width: 100%;\r\n    flex-direction: row;\r\n    align-items: center;\r\n  }\r\n\r\n  .camera-controls {\r\n    flex-direction: row;\r\n  }\r\n\r\n  .qr-code {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .qr-code h4 {\r\n    margin: 0;\r\n  }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .control-area {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .camera-controls {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .qr-code {\r\n    flex-direction: column;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}