AAY,MAAA,CAAA,cAAA,KAAmB;AAA/B,OAAA,CAIC;AAHN,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,MAAA,CAAA,YAAA,CAAa,MAAb,CAAF;AAAA;AAGC,KAPT,E,iBAMK,MAAA,CAAA,aAAA,CAAc,MAAd,C,CANL,EAMyB;AAAA;AANzB,MAMyB,WANzB,C;GAOS,CAPT,E;;AAAA,GAQI,EATN,CAUI,CAvBN,CA6BI,EAJJ,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,WAEI,EADJ,YAAA,CAAuE,kBAAvE,EAAuE;AAA9D,aAAM,gBAAwD;AAAtC,IAAA,MAAM,EAAE,MAAA,CAAA,eAA8B;AAAb,IAAA,UAAU,EAAV;AAAa,GAAvE,E,IAAA,E;;AAAA,I,UAAA,CACI,CAHN,CAII,CA9BN,CA2DK,EA3BL,mBAAA,CA0BM,KA1BN,EAAA,WAAA,EA0BM,CAzBJ,WAyBI,EAxBJ,mBAAA,CAuBM,KAvBN,EAAA,WAAA,EAuBM,CAtBJ,mBAAA,CAqBQ,OArBR,EAqBQ,IArBR,EAqBQ,CApBN,WAoBM,EAXN,mBAAA,CAUQ,OAVR,EAUQ,IAVR,EAUQ,E,kBATN,mBAAA,CAQK,SARL,EAQK,IARL,EAQK,WAAA,CARc,MAAA,CAAA,iBAQd,EAR+B,UAAzB,IAAyB,EAArB;yBAAf,mBAAA,CAQK,IARL,EAQK;AARkC,MAAA,GAAG,EAAE,IAAI,CAAC;AAQ5C,KARL,E,CACE,mBAAA,CAAwB,IAAxB,EAAwB,IAAxB,EAAwB,gBAAA,CAAjB,IAAI,CAAC,IAAY,CAAxB,EAAgB;AAAA;AAAhB,K,EACA,mBAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,IAAI,CAAC,OAAe,CAA3B,EAAmB;AAAA;AAAnB,K,EACA,mBAAA,CAA6B,IAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAtB,IAAI,CAAC,SAAiB,CAA7B,EAAqB;AAAA;AAArB,K,EACA,mBAAA,CAA6B,IAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAtB,IAAI,CAAC,SAAiB,CAA7B,EAAqB;AAAA;AAArB,K,EACA,mBAAA,CAEK,IAFL,EAAA,WAAA,EAEK,CADH,mBAAA,CAAkD,KAAlD,EAAA,WAAA,EAAkD,gBAAA,CAArB,IAAI,CAAC,OAAgB,CAAlD,EAAyC;AAAA;AAAzC,KACG,CAFL,C,CALF,C;GAQK,CARL,E;;AAAA,GASM,EAVR,CAWM,CArBR,CAsBI,CAvBN,CAwBI,CA1BN,CA2BK,CA/EP,CAgFI,CAzFN,C","sourcesContent":["<template>\n  <div class=\"full-container\">\n    <header>\n      <div class=\"nav-header\">\n        <router-link to=\"/\" class=\"nav-text\">时空维度·观天测地</router-link>\n        <span class=\"nav-text active\">星官体系·天人合一</span>\n        <router-link to=\"/analysis\" class=\"nav-text\">学术传承·道器相生</router-link>\n      </div>\n    </header>\n\n    <main class=\"main-content\">\n      <div class=\"status-cards\">\n        <div class=\"status-card\">\n          <h3>二十八宿</h3>\n          <div class=\"value\">28</div>\n        </div>\n        <div class=\"status-card\">\n          <h3>四象</h3>\n          <div class=\"value\">4</div>\n        </div>\n        <div class=\"status-card\">\n          <h3>星官</h3>\n          <div class=\"value\">283</div>\n        </div>\n        <div class=\"status-card\">\n          <h3>观测仪器</h3>\n          <div class=\"value\">12</div>\n        </div>\n      </div>\n\n      <div class=\"chart-area\">\n        <div class=\"star-chart-container\">\n          <h3>二十八宿星图</h3>\n          <div class=\"star-map-wrapper\">\n            <div class=\"star-map\" ref=\"starMapCanvas\"></div>\n            <div class=\"constellation-info\" v-if=\"selectedConstellation\">\n              {{ selectedConstellation.description }}\n            </div>\n            <div class=\"chart-controls\">\n              <button @click=\"zoomIn\">放大</button>\n              <button @click=\"zoomOut\">缩小</button>\n              <button @click=\"resetView\">重置视图</button>\n            </div>\n          </div>\n          <div class=\"season-selector\">\n            <button \n              v-for=\"season in ['春','夏','秋','冬']\" \n              :key=\"season\"\n              :class=\"{ active: selectedSeason === season }\"\n              @click=\"selectSeason(season)\"\n            >\n              {{ getSeasonName(season) }}\n            </button>\n          </div>\n        </div>\n\n        <div class=\"timeline-container\">\n          <h3>天文仪器发展时间轴</h3>\n          <v-chart class=\"timeline-chart\" :option=\"timelineOptions\" autoresize />\n        </div>\n      </div>\n\n      <div class=\"constellation-data\">\n        <h3>星官详细信息</h3>\n        <div class=\"data-table\">\n          <table>\n            <thead>\n              <tr>\n                <th>星官名称</th>\n                <th>所属宿</th>\n                <th>星数</th>\n                <th>对应地理</th>\n                <th>历史记载</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"item in constellationList\" :key=\"item.id\">\n                <td>{{ item.name }}</td>\n                <td>{{ item.mansion }}</td>\n                <td>{{ item.starCount }}</td>\n                <td>{{ item.geography }}</td>\n                <td class=\"history-cell\">\n                  <div class=\"history-text\">{{ item.history }}</div>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted, onUnmounted } from 'vue';\nimport { use } from 'echarts/core';\nimport { CanvasRenderer } from 'echarts/renderers';\nimport { PieChart, LineChart } from 'echarts/charts';\nimport { GridComponent, TooltipComponent, TitleComponent, LegendComponent, TimelineComponent } from 'echarts/components';\nimport VChart from 'vue-echarts';\nimport * as d3 from 'd3';\n\nuse([\n  CanvasRenderer,\n  PieChart,\n  LineChart,\n  GridComponent,\n  TooltipComponent,\n  TitleComponent,\n  LegendComponent,\n  TimelineComponent\n]);\n\nexport default {\n  name: 'StarSystemVisualization',\n  components: { VChart },\n  setup() {\n    // 数据状态\n    const selectedConstellation = ref(null);\n    const selectedSeason = ref('春');\n    const starMapCanvas = ref(null);\n    let zoom = null;\n    let svg = null;\n    let g = null;\n\n    // 星官数据\n    const constellationList = ref([\n      {\n        id: 1,\n        name: '角宿',\n        mansion: '东宫苍龙',\n        starCount: 12,\n        geography: '齐地',\n        history: '《史记·天官书》：角，天门，郊祀前望。'\n      },\n      // 其他星官数据...\n    ]);\n\n    // 时间轴配置\n    const timelineOptions = ref({\n      // 保持原有配置\n    });\n\n    // 方法\n    const getSeasonName = (season) => {\n      const names = { '春': '东宫苍龙', '夏': '南宫朱雀', '秋': '西宫白虎', '冬': '北宫玄武' };\n      return names[season];\n    };\n\n    const initStarMap = () => {\n      if (!starMapCanvas.value) return;\n\n      const container = starMapCanvas.value;\n      const { width, height } = container.getBoundingClientRect();\n      \n      // 清除现有内容\n      d3.select(container).selectAll(\"*\").remove();\n\n      // 创建SVG容器\n      svg = d3.select(container)\n        .append(\"svg\")\n        .attr(\"width\", width)\n        .attr(\"height\", height)\n        .style(\"background-color\", \"rgba(0, 0, 0, 0.5)\");\n\n      // 添加缩放功能\n      zoom = d3.zoom()\n        .scaleExtent([0.5, 5])\n        .on(\"zoom\", (event) => {\n          g.attr(\"transform\", event.transform);\n        });\n      \n      svg.call(zoom);\n\n      // 创建主绘图组\n      g = svg.append(\"g\");\n\n      // 星宿绘制逻辑（保持原有实现）\n      // ...\n    };\n\n    const selectSeason = (season) => {\n      selectedSeason.value = season;\n      initStarMap();\n    };\n\n    const zoomIn = () => {\n      if (svg) {\n        svg.transition().duration(300).call(zoom.scaleBy, 1.5);\n      }\n    };\n\n    const zoomOut = () => {\n      if (svg) {\n        svg.transition().duration(300).call(zoom.scaleBy, 0.75);\n      }\n    };\n\n    const resetView = () => {\n      if (svg) {\n        svg.transition().duration(500).call(zoom.transform, d3.zoomIdentity);\n      }\n    };\n\n    const handleResize = () => {\n      if (starMapCanvas.value && svg) {\n        const { width, height } = starMapCanvas.value.getBoundingClientRect();\n        svg.attr(\"width\", width).attr(\"height\", height);\n      }\n    };\n\n    // 生命周期\n    onMounted(() => {\n      initStarMap();\n      window.addEventListener('resize', handleResize);\n    });\n\n    onUnmounted(() => {\n      window.removeEventListener('resize', handleResize);\n    });\n\n    return {\n      selectedConstellation,\n      selectedSeason,\n      constellationList,\n      timelineOptions,\n      starMapCanvas,\n      getSeasonName,\n      selectSeason,\n      zoomIn,\n      zoomOut,\n      resetView\n    };\n  }\n};\n</script>\n\n<style scoped>\n/* 布局修正 */\n.full-container {\n  display: flex;\n  flex-direction: column;\n  height: 100vh;\n  overflow: hidden;\n}\n\n.main-content {\n  flex: 1;\n  overflow-y: auto;\n  padding: 20px;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.chart-area {\n  display: grid;\n  grid-template-columns: 2fr 1fr;\n  gap: 20px;\n  min-height: 600px;\n}\n\n.star-chart-container {\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n}\n\n.star-map-wrapper {\n  flex: 1;\n  position: relative;\n  min-height: 500px;\n  background: rgba(0, 0, 0, 0.3);\n  border-radius: 8px;\n  overflow: hidden;\n}\n\n.star-map {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n}\n\n/* 保持原有所有样式 */\n.nav-header {\n  height: 60px;\n  padding: 0 20px;\n  display: flex;\n  align-items: center;\n  gap: 30px;\n  background: rgba(13, 15, 44, 0.6);\n  position: relative;\n  border: 1px solid rgba(0, 195, 255, 0.3);\n}\n\n.nav-header::before,\n.nav-header::after {\n  content: '';\n  position: absolute;\n  width: 20px;\n  height: 20px;\n}\n\n.nav-header::before {\n  top: -1px;\n  left: -1px;\n  border-top: 2px solid #00c3ff;\n  border-left: 2px solid #00c3ff;\n}\n\n.nav-header::after {\n  bottom: -1px;\n  right: -1px;\n  border-bottom: 2px solid #00c3ff;\n  border-right: 2px solid #00c3ff;\n}\n\n.nav-text {\n  color: rgba(255, 255, 255, 0.8);\n  text-decoration: none;\n  font-size: 16px;\n  transition: all 0.3s ease;\n  position: relative;\n  padding: 5px 15px;\n}\n\n.nav-text:hover {\n  color: #00c3ff;\n}\n\n.nav-text.active {\n  color: #00c3ff;\n  background: rgba(0, 195, 255, 0.1);\n  border: 1px solid rgba(0, 195, 255, 0.3);\n}\n\n.status-cards {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  gap: 20px;\n}\n\n.status-card {\n  background: rgba(13, 15, 44, 0.7);\n  border-radius: 8px;\n  padding: 20px;\n  text-align: center;\n  border: 1px solid rgba(74, 144, 226, 0.6);\n  transition: all 0.3s ease;\n}\n\n.status-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 5px 15px rgba(74, 144, 226, 0.4);\n}\n\n.status-card h3 {\n  margin: 0;\n  font-size: 18px;\n  color: rgba(255, 255, 255, 0.9);\n}\n\n.value {\n  font-size: 32px;\n  font-weight: bold;\n  margin-top: 15px;\n  background: linear-gradient(45deg, #4a90e2, #64b5f6);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\n\n.constellation-info {\n  position: absolute;\n  bottom: 20px;\n  left: 20px;\n  right: 20px;\n  background: rgba(13, 15, 44, 0.9);\n  color: white;\n  padding: 15px;\n  border-radius: 4px;\n  font-size: 14px;\n  line-height: 1.6;\n  border: 1px solid rgba(74, 144, 226, 0.4);\n}\n\n.chart-controls {\n  position: absolute;\n  top: 20px;\n  right: 20px;\n  display: flex;\n  gap: 10px;\n  z-index: 10;\n}\n\n.season-selector {\n  display: flex;\n  gap: 10px;\n  margin-top: 15px;\n}\n\n.season-selector button {\n  flex: 1;\n  padding: 12px;\n  background: rgba(74, 144, 226, 0.2);\n  color: white;\n  border: 1px solid rgba(74, 144, 226, 0.4);\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.season-selector button.active {\n  background: rgba(74, 144, 226, 0.8);\n  border-color: #4a90e2;\n}\n\n.constellation-data {\n  background: rgba(13, 15, 44, 0.7);\n  border-radius: 8px;\n  padding: 20px;\n  margin-top: 20px;\n}\n\n.data-table {\n  overflow-x: auto;\n}\n\ntable {\n  width: 100%;\n  border-collapse: separate;\n  border-spacing: 0;\n}\n\nth, td {\n  padding: 12px 15px;\n  text-align: left;\n  border-bottom: 1px solid rgba(74, 144, 226, 0.2);\n}\n\nth {\n  font-weight: 500;\n  color: rgba(255, 255, 255, 0.9);\n  background: rgba(74, 144, 226, 0.1);\n}\n\n.history-cell {\n  max-width: 300px;\n}\n\n.history-text {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n.timeline-chart {\n  width: 100%;\n  height: 100%;\n}\n\n@keyframes twinkle {\n  0% { opacity: 0.3; }\n  50% { opacity: 1; }\n  100% { opacity: 0.3; }\n}\n\n.star-map::before {\n  content: '';\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  background-image: radial-gradient(circle at center, rgba(255,255,255,0.1) 0%, transparent 1%);\n  background-size: 15px 15px;\n  animation: twinkle 3s infinite;\n}\n</style>"],"sourceRoot":""}]}